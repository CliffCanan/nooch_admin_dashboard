@using noochAdminNew.Classes.ModelClasses
@using noochAdminNew.Classes.Utility
@using noochAdminNew.Models

@{
    ViewBag.Title = "Member Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
    MemberDetailsClass mdc = Model;

    SynapseDetailOFMember synpsDetail = new SynapseDetailOFMember();
    if (mdc.IsSynapseDetailAvailable)
    {
        synpsDetail = mdc.SynapseDetails;
    }
    List<MemberIpAddrreses> MemIpAddr = new List<MemberIpAddrreses>();
    MemIpAddr = mdc.MemberIpAddr;

    List<MemberDetailsTrans> Transactionsss = new List<MemberDetailsTrans>();

    List<Tenants> tn = new List<Tenants>();
    tn = mdc.tenants;

    foreach (MemberDetailsTrans mdtt in mdc.Transactions)
    {
        MemberDetailsTrans ms = new MemberDetailsTrans();
        ms.TransID = mdtt.TransID;
        ms.TransactionType = mdtt.TransactionType;
        ms.TransactionStatus = mdtt.TransactionStatus;
        ms.TransDate = mdtt.TransDate;
        ms.TransTime = mdtt.TransTime;
        ms.SenderId = mdtt.SenderId;
        ms.SenderName = mdtt.SenderName;
        ms.AmountNew = mdtt.AmountNew;
        ms.Latitude = mdtt.Latitude;
        ms.Longitude = mdtt.Longitude;
        ms.GeoLocation = mdtt.GeoLocation;
        ms.RecipientId = mdtt.RecipientId;
        ms.RecipientName = mdtt.RecipientName;
        ms.Memo = mdtt.Memo;
        ms.SynapseStatus = mdtt.SynapseStatus;
        ms.TransDatePaid = mdtt.TransDatePaid;
        Transactionsss.Add(ms);
    }

    List<MemberDetailsDisputeTrans> DisputeTransactions = Model.DisputeTransactions;
    MemberDetailsStats stats = Model.MemberStats;
}

<link rel="stylesheet" href="../css/plugins/jasny/jasny-bootstrap.min.css" />
<link href="https://gitcdn.github.io/bootstrap-toggle/2.2.2/css/bootstrap-toggle.min.css" rel="stylesheet">


<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-12">
        <div class="row">
            <div class="col-xs-9 col-sm-8 p-r-0">
                <h2><strong>@mdc.FirstName @mdc.LastName</strong> | <small>@mdc.memberId</small></h2>
            </div>
            <div class="col-xs-3 col-sm-4 p-l-0 text-right">
                <div class="m-t-25">@mdc.Nooch_ID</div>
            </div>
        </div>

        <ol class="breadcrumb">
            <li>
                <a href="../Admin/Dashboard">Dashboard</a>
            </li>
            <li>
                @{
                    if (mdc.type == "Landlord")
                    {
                        <a href="ListAllLandlords">Landlords</a>
                    }
                    else if (mdc.type == "Tenant")
                    {
                        <a href="ListAllTenants">Tenants</a>
                    }
                    else
                    {
                        <a href="ListAll">Members</a>
                    }
                }
            </li>
            <li class="active">
                <strong>Search Admin</strong>
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight" id="memberDetail">

    <div class="row" id="memDetailsTopSec">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>
                        @{
                            if (mdc.type == "Landlord")
                            {
                                <span>Landlord </span>
                            }
                            else if (mdc.type == "Tenant")
                            {
                                <span>TENANT </span>
                            }
                            else
                            {
                                <span>MEMBER </span>
                            }
                        }
                        Details | <small>@mdc.FirstName @mdc.LastName</small>
                    </h5>
                    @if (mdc.isRentScene == true)
                    {
                        <img src="https://noochme.com/noochweb/Assets/Images/rentscene.png" style="max-height:30px;margin-top:-6px;" alt="Rent Scene" data-toggle="tooltip" title="@mdc.FirstName @mdc.LastName is a Rent Scene user." />
                    }
                    <span class="pull-right" id="memId" data-val="@mdc.memberId">@mdc.memberId</span>
                    <div class="pull-right">
                        <a href="#" class="btn btn-link p-0 m-r-25" onclick="Member.OpenPopupForAdminNote();">
                            @if (mdc.adminNote != null)
                            {
                                <i class="fa fa-sticky-note" data-toggle="popover" data-trigger="click hover" data-title="Admin Note" data-content="@mdc.adminNote"></i>
                            }
                            else
                            {
                                <span data-toggle="tooltip" title="Add a note about @mdc.FirstName @mdc.LastName.">
                                    <i class="fa fa-plus f-10"></i>
                                    <i class="fa fa-sticky-note-o" title="@mdc.adminNote"></i>
                                </span>
                            }
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div class="row">

                        <div class="col-xs-12 col-md-4 m-b">
                            <div class="form-group">
                                <span class="col-xs-3 col-sm-4 col-md-5 control-label p-l-0">
                                    <img src="@mdc.ImageURL" class="img-circle" alt="image">
                                </span>
                                <div class="col-xs-9 col-sm-8 col-md-7 nopad">
                                    <h2>@mdc.FirstName @mdc.LastName</h2>
                                    <span class="help-block">Registered:<span class="text-bold">@mdc.DateCreatedFormatted</span> </span>
                                    <span class="help-block">
                                        Status:  @{
                                            if (mdc.Status == "Suspended" || mdc.Status == "Temporarily_Blocked")
                                            {
                                                <span class="label label-danger"><strong>Suspended</strong></span>
                                            }
                                            else if (mdc.Status == "Registered")
                                            {
                                                <strong class="label label-info">Registered</strong>
                                            }
                                            else if (mdc.Status == "Active")
                                            {
                                                <strong class="text-navy">Active</strong>
                                            }
                                            else
                                            {
                                                <span class="label label-danger">@mdc.Status</span>
                                            }
                                        }
                                    </span>
                                    <span class="help-block">
                                        Type:@{
                                            if (mdc.type != null)
                                            {
                                                <strong class="text-navy">@mdc.type</strong>
                                            }
                                            else
                                            {
                                                <strong class="label label-danger">-</strong>
                                            }
                                        }
                                    </span>
                                    <span class="help-block">
                                        Phone Verified:@{
                                            if (mdc.IsPhoneVerified == true)
                                            {
                                                <span class="label label-success">Verified</span>
                                            }
                                            else
                                            {
                                                <strong class="label label-danger">Not Verified</strong>
                                            }
                                        }
                                    </span>
                                    @if (!String.IsNullOrEmpty(mdc.ReferCodeUsed))
                                    {
                                        <span class="help-block m-b-none">Refer Code Used:<a href="../Referrals/JoinningsWithGivenReferralCode?RefCode=@mdc.ReferCodeUsed" class="text-navy">@mdc.ReferCodeUsed.ToUpper()</a></span>
                                    }
                                    else
                                    {
                                        <span class="help-block m-b-none">Refer Code Used: <em>None</em></span>
                                    }
                                </div>
                            </div>
                        </div>

                        <!-- Transfer Activity -->
                        <div id="transSummaryBox" class="col-xs-12 col-sm-6 col-md-4">
                            <div class="ibox float-e-margins" style="margin-bottom:5px;">
                                <div class="ibox-title">
                                    <h5>Transfer Activity</h5>
                                </div>
                                <div class="ibox-content text-center">
                                    <div class="row">
                                        <div class="col-xs-5 col-xs-offset-1">
                                            <small class="stats-label">Total Transfers</small>
                                            <h4>@stats.TotalTransfer</h4>
                                        </div>

                                        <div class="col-xs-5">
                                            <small class="stats-label">Transfers / Wk</small>
                                            @if (mdc.WeeksSinceJoined > 0 && mdc.MemberStats.TotalTransfer != "0")
                                            {
                                                double d = (Convert.ToDouble(mdc.MemberStats.TotalTransfer)) / mdc.WeeksSinceJoined;
                                                string transPerWeek = string.Format("{0:0.00}", d);
                                                <h4>@transPerWeek</h4>
                                            }
                                            else
                                            {<h4>-</h4>}
                                        </div>
                                    </div>
                                </div>
                                <div class="ibox-content text-center">
                                    <div class="row">

                                        <div class="col-xs-4">
                                            <small class="stats-label">Sent</small>
                                            <h4>$ @stats.TotalSent</h4>
                                        </div>
                                        <div class="col-xs-4">
                                            <small class="stats-label">Received</small>
                                            <h4>$ @stats.TotalReceived</h4>
                                        </div>
                                        <div class="col-xs-4">
                                            <small class="stats-label">Largest Sent</small>
                                            <h4>
                                                @if (stats.LargestSent != "0.00")
                                                {
                                                    <span>$ @stats.LargestSent</span>
                                                }
                                                else
                                                {
                                                    <span>-</span>
                                                }
                                            </h4>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>

                        <!-- Member Ip Address-->
                        <div class="col-xs-12 col-md-4">
                            <div class="ibox float-e-margins m-b-0">
                                <div class="ibox-title">
                                    <ul class="nav nav-tabs small">
                                        <li class="active"><a data-toggle="tab" href="#tab-a"><i class="fa fa-fw fa-crosshairs m-r-xs"></i> IP Addresses</a></li>
                                        <li class=""><a data-toggle="tab" href="#tab-b"><i class="fa fa-fw fa-map-marker m-r-xs"></i> Location</a></li>
                                    </ul>
                                </div>

                                <div class="ibox-content p-b-0">
                                    <div class="tab-content ">

                                        <div id="tab-a" class="tab-pane fade active in">
                                            @{
                                                if (MemIpAddr.Count > 0)
                                                {
                                                    <table class="table table-striped">
                                                        <thead>
                                                            <tr>
                                                                <th>Ip Address</th>
                                                                <th>Date</th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            @foreach (MemberIpAddrreses mn in MemIpAddr)
                                                            {
                                                                <tr>
                                                                    <td>@mn.IpAddress</td>
                                                                    <td>@mn.Date</td>
                                                                </tr>
                                                            }
                                                        </tbody>
                                                    </table>
                                                }
                                                else
                                                {
                                                    <p class="text-danger text-center m-t-md"> There are no IP Addresses for<em>@mdc.FirstName @mdc.LastName</em> </p>
                                                }
                                            }
                                        </div>

                                        <div id="tab-b" class="tab-pane fade">
                                            @{
                                                <div id="userLoc" data-lat="@mdc.lastlat" data-long="@mdc.lastlong">
                                                    <div id="userMap"></div>
                                                </div>
                                            }
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

