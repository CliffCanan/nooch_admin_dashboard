``@{
    ViewBag.Title = "UsersOverTime";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<section id="content">
    <div class="container">
       
        <!-- Modal -->
        <div class="modal fade" id="myModal" role="dialog">
            <div class="modal-dialog">

                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title">Date Range</h4>
                    </div>
                    <div class="modal-body">
                        <div class="showDate hide">
                            <form id="frmTarget" method="get" data-parsley-validate="" onsubmit="return false;">
                                Start Date  <input readonly data-parsley-required="true" data-parsley-required-message="Date is required!" type="text" id="startDate" class="date" />  End Date <input type="text" readonly id="endDate" data-parsley-required="true" data-parsley-required-message="Date is required!" class="date" />
                                <button id="btnShow" type="submit" class="btn-info" onclick="Member.getUserOverTime('dateRange');">show</button>
                                
                            </form>
                        </div>
                    </div>

                </div>

            </div>
        </div>
        <div class="row">
           

            <div class="col-md-11">
                <div class="card">
                    <div class="card-header">
                        <h2 id="headerUserOverTime">Bar Chart for showing Graph of users over time</h2>

                        <ul class="actions">
                            <li class="dropdown action-show">
                                <a href="" data-toggle="dropdown">
                                    <i class="md md-more-vert">More..</i>
                                </a>

                                <div class="dropdown-menu pull-right">
                                    <ul>
                                        <li><a onclick="Member.getUserOverTime('daily');">Daily</a></li>
                                        <li><a onclick="Member.getUserOverTime('weekly');">weekly</a></li>
                                        <li><a onclick="Member.getUserOverTime('monthly');">monthly</a></li>
                                        <li><a onclick="Member.getUserOverTime('yearly');">yearly</a></li>
                                        <li><a data-toggle="modal" data-target="#myModal" onclick="Member.showDate( );">Date range</a></li>

                                    </ul>
                                </div>
                            </li>
                        </ul>
                    </div>
                    

                    <div class="card-body card-padding-sm">
                        <div id="bar-chart" class="flot-chart"></div>
                        <div class="flc-bar"></div>
                        <input type="radio" required name="status" checked="checked" value="0" onclick="Member.getUserOverTime('r');" />Registered
                        <input type="radio" required name="status" value="1" onclick="Member.getUserOverTime('r');" />Non-Registered
                       
                     </div>
                    
                </div>
            </div>

           

           
    </div>
        


 </div>
</section>
 
        

   <!-- Javascript Libraries -->
 

 
<script src="~/Scripts/jsDatePick.min.1.3.js"></script>
<script src="~/Scripts/App/flot/jquery.flot.js"></script>
 
 
 
<script src="~/Scripts/App/UserOverTime.js"></script>
<link href="~/css/jsDatePick_ltr.css" rel="stylesheet" />
<link href="~/css/jsDatePick_ltr.min.css" rel="stylesheet" />
 
 
 

